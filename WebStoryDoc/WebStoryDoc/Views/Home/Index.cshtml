@using WebStoryDoc.Models
@model HomeModel
@{
    ViewBag.Title = Model.Title;
}

<div class="jumbotron">
    <h1>@Model.Time</h1>
</div>

@using (Html.BeginForm())
{
<div>
    @Html.DisplayNameFor(m => m.Title)
</div>
<div>
    @Html.EditorFor(m => m.Title)
    @Html.HiddenFor(m => m.Time)
</div>
    <div>
        <input type="submit" value="Отправить" />
    </div>
}