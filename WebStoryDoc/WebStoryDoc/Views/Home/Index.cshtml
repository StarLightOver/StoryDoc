@using WebStoryDoc.Models
@model HomeModel
@{
    ViewBag.Title = Model.Title;
}

<div class="jumbotron">
    <h1>@Model.Time</h1>
</div>

@using (Html.BeginForm())
{
    <div>
        @Html.DisplayNameFor(m => m.Title)
    </div>
    <br />
    <div>
        @Html.EditorFor(m => m.Title)
        @Html.HiddenFor(m => m.Time)
    </div>
    <br />
    <div>
        <input type="submit" value="Отправить" />
    </div>

    @Html.ActionLink("Вывести список пользователей", "List", "User")
    <br />
    @Html.ActionLink("Вывести список документов", "List", "Folder")
}